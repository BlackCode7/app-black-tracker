package br.com.blackcode.appBlackTraker.AppBlackTrakerJava;

import static org.junit.jupiter.api.Assertions.assertEquals;

import org.junit.jupiter.api.DisplayName;
import org.junit.jupiter.api.Order;
import org.junit.jupiter.api.Test;

import br.com.blackcode.appBlackTraker.model.LogonUsuariosModel;

public class LogonUsuariosModelTest {

	@Test
	@Order(1)
	@DisplayName("Testando os tipos campos da classe")
	void TestaOsTiposDaClasseLogonUsuariosModel() {
		LogonUsuariosModel varId = LogonUsuariosModel.builder()
				.id((long) 1)
				.build() ;
		assertEquals(true, varId.getId());
	}
}
